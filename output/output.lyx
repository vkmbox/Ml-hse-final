\begin{equation}
f(x)=\mathrm{sign}(\sum{}_{k=1}^{T}\alpha_{k}h_{k}(x)),\alpha_{k}\ge0.\label{eq:adaboost_ensemble}
\end{equation}

\end_inset


\end_layout

