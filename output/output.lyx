\begin_inset Formula 
\begin{equation}
\hat{R}_{S,œÅ}(h)=\frac{1}{m}\sum{}_{t\text{=1}}^{m}ùõ∑_{œÅ}(Œ±¬∑h(x_{t}))\label{eq:mg_loss_sep}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\noindent
A comparison of formulae 
